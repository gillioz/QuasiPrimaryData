Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"gseriescoefficient", "::", "outofrange"}], "=", 
   "\"\<Error: parameters (n, l, i, j) = (`1`, `2`, `3`, `4`) out of \
range\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"gseriescoefficient", "::", "indeterminate"}], "=", 
   "\"\<Error: impossible to perform the Taylor expansion of the conformal \
block\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"gseriescoefficient", "[", 
    RowBox[{
    "\[CapitalDelta]_", ",", "n_", ",", "l_", ",", "\[CapitalDelta]12_"}], 
    "]"}], "[", 
   RowBox[{"i_", ",", "j_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"n", "<", "0"}], "||", 
        RowBox[{"l", "<", "0"}], "||", 
        RowBox[{"l", ">", "n"}], "||", 
        RowBox[{"i", "<", "0"}], "||", 
        RowBox[{"j", "<", "0"}], "||", 
        RowBox[{"j", ">", "i"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Message", "[", 
         RowBox[{
          RowBox[{"gseriescoefficient", "::", "outofrange"}], ",", "n", ",", 
          "l", ",", "i", ",", "j"}], "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"Return", "[", "0", "]"}]}]}], "\[IndentingNewLine]", "]"}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"\[CapitalDelta]", "\[Equal]", "0"}], "&&", 
        RowBox[{"\[CapitalDelta]12", "\[NotEqual]", "0"}]}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{
         RowBox[{
          RowBox[{"if", " ", "\[CapitalDelta]"}], " ", "=", " ", 
          RowBox[{
          "0", " ", "the", " ", "intermediate", " ", "operator", " ", "is", 
           " ", "in", " ", "the", " ", "identity", " ", "Virasoro", " ", 
           "block"}]}], ",", " ", 
         RowBox[{
          RowBox[{
          "so", " ", "we", " ", "must", " ", "have", " ", 
           "\[CapitalDelta]12"}], " ", "=", " ", "0"}]}], " ", "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Message", "[", 
         RowBox[{"gseriescoefficient", "::", "indeterminate"}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"Return", "[", "Null", "]"}]}]}], "\[IndentingNewLine]", 
      "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"\[CapitalDelta]", "\[Equal]", "0"}], "&&", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"l", "\[Equal]", "0"}], "||", 
          RowBox[{"l", "\[Equal]", "n"}]}], ")"}]}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"n", "\[Equal]", "0"}], ",", "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"i", "\[Equal]", "0"}], ",", "1", ",", "0"}], "]"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"j", "\[Equal]", "0"}], "||", 
              RowBox[{"j", "\[Equal]", "i"}]}], ")"}], "&&", 
            RowBox[{"i", "\[GreaterEqual]", "n"}]}], ",", 
           FractionBox[
            SuperscriptBox[
             RowBox[{"Pochhammer", "[", 
              RowBox[{"n", ",", 
               RowBox[{"i", "-", "n"}]}], "]"}], "2"], 
            RowBox[{
             RowBox[{
              RowBox[{"(", 
               RowBox[{"i", "-", "n"}], ")"}], "!"}], 
             RowBox[{"Pochhammer", "[", 
              RowBox[{
               RowBox[{"2", "n"}], ",", 
               RowBox[{"i", "-", "n"}]}], "]"}]}]], ",", "0"}], "]"}]}], 
        "\[IndentingNewLine]", "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"2", "l"}], "\[Equal]", "n"}], ",", "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"i", "-", "j"}], "\[GreaterEqual]", "l"}], "&&", 
            RowBox[{"j", "\[GreaterEqual]", "l"}]}], ",", 
           FractionBox[
            RowBox[{
             SuperscriptBox[
              RowBox[{"Pochhammer", "[", 
               RowBox[{
                RowBox[{
                 FractionBox[
                  RowBox[{"\[CapitalDelta]", "-", "\[CapitalDelta]12"}], "2"],
                  "+", "l"}], ",", 
                RowBox[{"j", "-", "l"}]}], "]"}], "2"], 
             SuperscriptBox[
              RowBox[{"Pochhammer", "[", 
               RowBox[{
                RowBox[{
                 FractionBox[
                  RowBox[{"\[CapitalDelta]", "-", "\[CapitalDelta]12"}], "2"],
                  "+", "l"}], ",", 
                RowBox[{"i", "-", "j", "-", "l"}]}], "]"}], "2"]}], 
            RowBox[{
             RowBox[{
              RowBox[{"(", 
               RowBox[{"j", "-", "l"}], ")"}], "!"}], 
             RowBox[{
              RowBox[{"(", 
               RowBox[{"i", "-", "j", "-", "l"}], ")"}], "!"}], 
             RowBox[{"Pochhammer", "[", 
              RowBox[{
               RowBox[{"\[CapitalDelta]", "+", 
                RowBox[{"2", "l"}]}], ",", 
               RowBox[{"j", "-", "l"}]}], "]"}], 
             RowBox[{"Pochhammer", "[", 
              RowBox[{
               RowBox[{"\[CapitalDelta]", "+", 
                RowBox[{"2", "l"}]}], ",", 
               RowBox[{"i", "-", "j", "-", "l"}]}], "]"}]}]], ",", "0"}], 
          "]"}], ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{"i", "-", "j"}], "\[GreaterEqual]", "l"}], "&&", 
             RowBox[{"j", "\[GreaterEqual]", 
              RowBox[{"n", "-", "l"}]}]}], ",", 
            FractionBox[
             RowBox[{
              SuperscriptBox[
               RowBox[{"Pochhammer", "[", 
                RowBox[{
                 RowBox[{
                  FractionBox[
                   RowBox[{"\[CapitalDelta]", "-", "\[CapitalDelta]12"}], 
                   "2"], "+", "n", "-", "l"}], ",", 
                 RowBox[{"j", "-", "n", "+", "l"}]}], "]"}], "2"], 
              SuperscriptBox[
               RowBox[{"Pochhammer", "[", 
                RowBox[{
                 RowBox[{
                  FractionBox[
                   RowBox[{"\[CapitalDelta]", "-", "\[CapitalDelta]12"}], 
                   "2"], "+", "l"}], ",", 
                 RowBox[{"i", "-", "j", "-", "l"}]}], "]"}], "2"]}], 
             RowBox[{
              RowBox[{
               RowBox[{"(", 
                RowBox[{"j", "-", "n", "+", "l"}], ")"}], "!"}], 
              RowBox[{
               RowBox[{"(", 
                RowBox[{"i", "-", "j", "-", "l"}], ")"}], "!"}], 
              RowBox[{"Pochhammer", "[", 
               RowBox[{
                RowBox[{"\[CapitalDelta]", "+", 
                 RowBox[{"2", "n"}], "-", 
                 RowBox[{"2", "l"}]}], ",", 
                RowBox[{"j", "-", "n", "+", "l"}]}], "]"}], 
              RowBox[{"Pochhammer", "[", 
               RowBox[{
                RowBox[{"\[CapitalDelta]", "+", 
                 RowBox[{"2", "l"}]}], ",", 
                RowBox[{"i", "-", "j", "-", "l"}]}], "]"}]}]], ",", "0"}], 
           "]"}], "+", 
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{"i", "-", "j"}], "\[GreaterEqual]", 
              RowBox[{"n", "-", "l"}]}], "&&", 
             RowBox[{"j", "\[GreaterEqual]", "l"}]}], ",", 
            FractionBox[
             RowBox[{
              SuperscriptBox[
               RowBox[{"Pochhammer", "[", 
                RowBox[{
                 RowBox[{
                  FractionBox[
                   RowBox[{"\[CapitalDelta]", "-", "\[CapitalDelta]12"}], 
                   "2"], "+", "n", "-", "l"}], ",", 
                 RowBox[{"i", "-", "j", "-", "n", "+", "l"}]}], "]"}], "2"], 
              SuperscriptBox[
               RowBox[{"Pochhammer", "[", 
                RowBox[{
                 RowBox[{
                  FractionBox[
                   RowBox[{"\[CapitalDelta]", "-", "\[CapitalDelta]12"}], 
                   "2"], "+", "l"}], ",", 
                 RowBox[{"j", "-", "l"}]}], "]"}], "2"]}], 
             RowBox[{
              RowBox[{
               RowBox[{"(", 
                RowBox[{"i", "-", "j", "-", "n", "+", "l"}], ")"}], "!"}], 
              RowBox[{
               RowBox[{"(", 
                RowBox[{"j", "-", "l"}], ")"}], "!"}], 
              RowBox[{"Pochhammer", "[", 
               RowBox[{
                RowBox[{"\[CapitalDelta]", "+", 
                 RowBox[{"2", "n"}], "-", 
