Cell[TextData[{
 "For each Virasoro primary operator O, the function ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["H", "O"], "(", "z", ")"}], 
    RowBox[{
     SubscriptBox["H", "O"], "(", 
     OverscriptBox["z", "_"], ")"}]}], TraditionalForm]],ExpressionUUID->
  "9c84a187-5c45-4412-b933-ac5f3e97536b"],
 " can be expanded in a sum over quasi-primary operators ",
 Cell[BoxData[
  FormBox["\[Psi]", TraditionalForm]],ExpressionUUID->
  "6aa0b2c7-a21e-426e-b868-79b4b0f06a3e"],
 " with dimension \[CapitalDelta] and spin j as\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      SubscriptBox["H", "O"], "(", "z", ")"}], 
     RowBox[{
      SubscriptBox["H", "O"], "(", 
      OverscriptBox["z", "_"], ")"}]}], "=", 
    RowBox[{"\[Sum]", 
     RowBox[{
      SubscriptBox["\[Lambda]", 
       RowBox[{"12", "\[Psi]"}]], 
      SubscriptBox["\[Lambda]", 
       RowBox[{"34", "\[Psi]"}]], 
      RowBox[{
       SubscriptBox["G", 
        RowBox[{"\[CapitalDelta]", ",", "j"}]], "(", 
       RowBox[{"z", ",", 
        OverscriptBox["z", "_"]}], ")"}]}]}]}], TraditionalForm]],
  ExpressionUUID->"471183a3-97e2-4ed8-a318-f4b0a4556c80"],
 "\nwhere\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["G", 
      RowBox[{"\[CapitalDelta]", ",", "0"}]], "(", 
     RowBox[{"z", ",", 
      OverscriptBox["z", "_"]}], ")"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["k", "\[CapitalDelta]"], "(", "z", ")"}], 
     RowBox[{
      SubscriptBox["k", "\[CapitalDelta]"], "(", 
      OverscriptBox["z", "_"], ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "8b1cd541-5918-4934-9fad-0868aa4bc998"],
 "\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["G", 
      RowBox[{"\[CapitalDelta]", ",", "j"}]], "(", 
     RowBox[{"z", ",", 
      OverscriptBox["z", "_"]}], ")"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       SubscriptBox["k", 
        RowBox[{"\[CapitalDelta]", "-", "j"}]], "(", "z", ")"}], 
      RowBox[{
       SubscriptBox["k", 
        RowBox[{"\[CapitalDelta]", "+", "j"}]], "(", 
       OverscriptBox["z", "_"], ")"}]}], "+", 
     RowBox[{
      RowBox[{
       SubscriptBox["k", 
        RowBox[{"\[CapitalDelta]", "+", "j"}]], "(", "z", ")"}], 
      RowBox[{
       SubscriptBox["k", 
        RowBox[{"\[CapitalDelta]", "-", "j"}]], "(", 
       OverscriptBox["z", "_"], ")"}]}]}]}], TraditionalForm]],ExpressionUUID->
  "5846f787-3406-4d25-9c26-b477a473ee20"],
 "\t\t(",
 Cell[BoxData[
  FormBox[
   RowBox[{"j", ">", "0"}], TraditionalForm]],ExpressionUUID->
  "8d873462-7a55-42b1-a167-94acb2e1da07"],
 ")\nand\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["k", "\[Beta]"], "(", "z", ")"}], "=", 
    RowBox[{
     SuperscriptBox["z", 
      RowBox[{"\[Beta]", "/", "2"}]], " ", 
     RowBox[{
      SubscriptBox["\[InvisiblePrefixScriptBase]", "2"], 
      SubscriptBox["F", "1"]}], 
     RowBox[{"(", 
      RowBox[{
       FractionBox[
        RowBox[{"\[Beta]", "-", 
         SubscriptBox["\[CapitalDelta]", "1"], "+", "\[CapitalDelta]2"}], 
        "2"], ",", 
       FractionBox[
        RowBox[{"\[Beta]", "+", 
         SubscriptBox["\[CapitalDelta]", "3"], "-", 
         SubscriptBox["\[CapitalDelta]", "4"]}], "2"], ",", "\[Beta]", ",", 
       "z"}], ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "ea282b7c-430c-4e32-9ddb-66fa2ab2372f"]
