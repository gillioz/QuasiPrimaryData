Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"QuasiPrimaryData", "::", "failure"}], "=", 
   "\"\<Error: unable to expand the Virsoro block in global conformal blocks.\
\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"QuasiPrimaryData", "[", 
   RowBox[{
   "\[CapitalDelta]1_", ",", "\[CapitalDelta]2_", ",", "\[CapitalDelta]_", 
    ",", "H_", ",", "\[CapitalDelta]max_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "nmax", ",", "Hseries", ",", "Gcoefficients", ",", "\[Epsilon]", ",", 
      "data", ",", "n", ",", "l", ",", "\[Lambda]", ",", "i", ",", "j"}], 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"Head", "[", "\[CapitalDelta]", "]"}], "===", "List"}], "&&", 
        RowBox[{
         RowBox[{"Head", "[", "H", "]"}], "===", "List"}], "&&", 
        RowBox[{
         RowBox[{"Length", "[", "\[CapitalDelta]", "]"}], "\[Equal]", 
         RowBox[{"Length", "[", "H", "]"}]}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"Return", "[", 
        RowBox[{"CombineOPEData", "[", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"QuasiPrimaryData", "[", 
            RowBox[{"\[CapitalDelta]1", ",", "\[CapitalDelta]2", ",", 
             RowBox[{"\[CapitalDelta]", "[", 
              RowBox[{"[", "i", "]"}], "]"}], ",", 
             RowBox[{"H", "[", 
              RowBox[{"[", "i", "]"}], "]"}], ",", "\[CapitalDelta]max"}], 
            "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", 
             RowBox[{"Length", "[", "\[CapitalDelta]", "]"}]}], "}"}]}], 
          "]"}], "]"}], "]"}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"nmax", "=", 
      RowBox[{"Floor", "[", 
       RowBox[{"\[CapitalDelta]max", "-", "\[CapitalDelta]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Hseries", "=", 
      RowBox[{"Series", "[", 
       RowBox[{
        RowBox[{
         SuperscriptBox["\[Epsilon]", 
          RowBox[{
           RowBox[{"-", "\[CapitalDelta]"}], "/", "2"}]], 
         RowBox[{"H", "[", "\[Epsilon]", "]"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"\[Epsilon]", ",", "0", ",", "nmax"}], "}"}]}], "]"}]}], ";",
      "\[IndentingNewLine]", 
     RowBox[{"Gcoefficients", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"SeriesCoefficient", "[", 
          RowBox[{"Hseries", ",", "j"}], "]"}], 
         RowBox[{"SeriesCoefficient", "[", 
          RowBox[{"Hseries", ",", 
           RowBox[{"n", "-", "j"}]}], "]"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"n", ",", "0", ",", "nmax"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"j", ",", "0", ",", 
          FractionBox["n", "2"]}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"data", "=", 
      RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"n", "=", "0"}], ",", 
       RowBox[{"n", "\[LessEqual]", "nmax"}], ",", 
       RowBox[{"n", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"For", "[", 
         RowBox[{
          RowBox[{"l", "=", "0"}], ",", 
          RowBox[{"l", "\[LessEqual]", 
           FractionBox["n", "2"]}], ",", 
          RowBox[{"l", "++"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"\[Lambda]", "=", 
            RowBox[{"Gcoefficients", "[", 
             RowBox[{"[", 
              RowBox[{
               RowBox[{"n", "+", "1"}], ",", 
               RowBox[{"l", "+", "1"}]}], "]"}], "]"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"\[Lambda]", "\[NotEqual]", "0"}], ",", 
             "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"AppendTo", "[", 
               RowBox[{"data", ",", 
                RowBox[{"{", 
                 RowBox[{
                  RowBox[{"\[CapitalDelta]", "+", "n"}], ",", 
                  RowBox[{"n", "-", 
                   RowBox[{"2", "l"}]}], ",", "\[Lambda]"}], "}"}]}], "]"}], 
              ";", "\[IndentingNewLine]", 
              RowBox[{"For", "[", 
               RowBox[{
                RowBox[{"i", "=", "n"}], ",", 
                RowBox[{"i", "\[LessEqual]", "nmax"}], ",", 
                RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
                RowBox[{"For", "[", 
                 RowBox[{
                  RowBox[{"j", "=", "0"}], ",", 
                  RowBox[{"j", "\[LessEqual]", 
                   FractionBox["i", "2"]}], ",", 
                  RowBox[{"j", "++"}], ",", "\[IndentingNewLine]", 
                  RowBox[{
                   RowBox[{"Gcoefficients", "[", 
                    RowBox[{"[", 
                    RowBox[{
                    RowBox[{"i", "+", "1"}], ",", 
                    RowBox[{"j", "+", "1"}]}], "]"}], "]"}], "-=", 
                   RowBox[{"\[Lambda]", " ", 
                    RowBox[{
                    RowBox[{"gseriescoefficient", "[", 
                    RowBox[{"\[CapitalDelta]", ",", "n", ",", "l", ",", 
                    RowBox[{"\[CapitalDelta]1", "-", "\[CapitalDelta]2"}]}], 
                    "]"}], "[", 
                    RowBox[{"i", ",", "j"}], "]"}]}]}]}], 
                 "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]", 
               "]"}]}]}], "\[IndentingNewLine]", "]"}]}]}], 
         "\[IndentingNewLine]", "]"}], ";"}]}], "\[IndentingNewLine]", "]"}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"DeleteDuplicates", "[", 
         RowBox[{"Flatten", "[", "Gcoefficients", "]"}], "]"}], "=!=", 
        RowBox[{"{", "0", "}"}]}], ",", 
       RowBox[{"Message", "[", 
        RowBox[{"QuasiPrimaryData", "::", "failure"}], "]"}]}], "]"}], ";", 
     "\[IndentingNewLine]", "data"}]}], "\[IndentingNewLine]", 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"QuasiPrimaryData", "[", 
