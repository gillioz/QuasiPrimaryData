Cell[TextData[{
 "The conformal blocks admit a series expansion in the form\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["G", 
      RowBox[{"\[CapitalDelta]", ",", "j"}]], "(", 
     RowBox[{
      RowBox[{"r", " ", 
       SuperscriptBox["e", 
        RowBox[{
         RowBox[{"-", "i"}], " ", "\[Phi]"}]]}], ",", 
      RowBox[{"r", " ", 
       SuperscriptBox["e", 
        RowBox[{"i", " ", "\[Phi]"}]]}]}], ")"}], "=", 
    RowBox[{
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{"n", "=", "0"}], "\[Infinity]"], 
     UnderoverscriptBox["\[CapitalSigma]", 
      RowBox[{"m", "=", 
       RowBox[{
        RowBox[{"-", "j"}], "-", "n"}]}], 
      RowBox[{"j", "+", "n"}]], 
     SuperscriptBox["r", 
      RowBox[{"\[CapitalDelta]", "+", "n"}]], 
     SuperscriptBox["e", 
      RowBox[{"i", " ", "\[Phi]", " ", "m"}]], 
     RowBox[{
      SubscriptBox["c", 
       RowBox[{"n", ",", "m"}]], "(", 
      RowBox[{"\[CapitalDelta]", ",", "j"}], ")"}]}]}], TraditionalForm]],
  ExpressionUUID->"b31152d6-5679-4bbd-bd79-6fbcafc250fb"]
}], "Text",
 CellChangeTimes->{{3.757945806582624*^9, 3.757945818934149*^9}, {
